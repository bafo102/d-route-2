{
    "manifest_version": 3,
    "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
    },
    "action": {
        "default_icon": "icon.png"    
    },
    "name": "D-route 2",
    "version": "1.0",
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "permissions": ["favicon"],
    "web_accessible_resources": [
        {
        "resources": ["_favicon/*"],
        "matches": ["<all_urls>"],
        "extension_ids": ["*"]
        }
    ]
}